{
    "rewrites": [
        {
            "source": "/app",
            "destination": "https://kader-app.vercel.app/app/"
        },
        {
            "source": "/app/:match*",
            "destination": "https://kader.vercel.app/app/:match*"
        },
        { "source": "/", "destination": "https://kader-web.vercel.app/" },
        {
            "source": "/:match*",
            "destination": "https://kader.vercel.app/:match*"
        }
    ]
}
